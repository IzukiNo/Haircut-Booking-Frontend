{
  "version": 2,
  "rewrites": [
    { "source": "/about", "destination": "/about.html" },
    { "source": "/auth", "destination": "/auth.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/services", "destination": "/services.html" },
    {
      "source": "/dashboard/users",
      "destination": "/dashboard/dashboard-users.html"
    },
    {
      "source": "/dashboard/settings",
      "destination": "/dashboard/dashboard-settings.html"
    },
    {
      "source": "/dashboard/appointments",
      "destination": "/dashboard/dashboard-appointments.html"
    },
    {
      "source": "/dashboard/services",
      "destination": "/dashboard/dashboard-services.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy.js"
    }
  ]
}
